{
  "name": "pupetteer_starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"./**/*.{ts, js, json, md}\"",
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "pre": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "t:u": "mocha -r ts-node/register test/unit-tests/**/*.spec.ts",
    "t:u:w": "npm run t:u -- --w",
    "test:watch": "mocha -r ts-node/register test/**/test.ts",
    "forever": "forever start -a -l NUL dist/main.js",
    "list": "forever list",
    "s": "forever stop 0",
    "log": "forever logs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "^5.2.6",
    "@types/puppeteer": "^1.12.1",
    "@types/puppeteer-core": "^1.9.0",
    "dotenv": "^6.2.0",
    "expect": "^24.1.0",
    "expect-puppeteer": "^3.5.1",
    "forever": "^0.15.3",
    "inversify": "^5.0.1",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.11",
    "puppeteer-core": "^1.11.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/expect": "^1.20.4",
    "@types/jsdom": "^12.2.2",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.3.17",
    "@types/node": "^10.12.21",
    "@types/request": "^2.48.1",
    "mockdate": "^2.0.2",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sinon": "^7.2.3",
    "testdouble": "^3.10.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  }
}
